<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="407.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TournamentController">
   <children>
      <Button fx:id="startbutton" mnemonicParsing="false" onMouseClicked="#start_game" prefHeight="39.0" prefWidth="144.0" text="Start" StackPane.alignment="CENTER" />
      <Button fx:id="mainmenu" mnemonicParsing="false" onMouseClicked="#change_menu" prefHeight="35.0" prefWidth="88.0" text="Back" StackPane.alignment="TOP_RIGHT">
         <StackPane.margin>
            <Insets right="20.0" top="20.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="savebutton" mnemonicParsing="false" onMouseClicked="#save" prefHeight="35.0" prefWidth="88.0" text="Save" visible="false" StackPane.alignment="BOTTOM_LEFT">
         <StackPane.margin>
            <Insets bottom="20.0" left="20.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
